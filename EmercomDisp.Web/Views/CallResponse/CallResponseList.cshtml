@model EmercomDisp.Web.Models.CallResponses.CallResponseListModel

<div class="panel" style="width:100%">
    <table class="table table-striped table-hover table-responsive table-condensed">
        <tr>
            <th>
                <h4 style="font-size:x-large"><span style="font-weight:bolder">Brigade Name</span></h4=>
            </th>
            <th>
                <h4 style="font-size:x-large"><span style="font-weight:bolder">Transfer Time</span></h4>
            </th>
            <th>
                <h4 style="font-size:x-large"><span style="font-weight:bolder">Arrive Time</span></h4>
            </th>
            <th>
                <h4 style="font-size:x-large"><span style="font-weight:bolder">Finish Time</span></h4>
            </th>
            <th>
                <h4 style="font-size:x-large"><span style="font-weight:bolder">Return Time</span></h4>
            </th>
        </tr>

        @foreach (var callResponse in Model.CallResponses)
        {
        <tr>
            <td class="col-lg-2">
                <span style="font-size: 14px;">@Html.DisplayFor(model => callResponse.BrigadeName)</span>
            </td>
            <td class="col-lg-2">
                <span style="font-size: 14px;">@Html.DisplayFor(model => callResponse.TransferTime)</span>
            </td>
            <td class="col-lg-2">
                <span style="font-size: 14px;">@Html.DisplayFor(model => callResponse.ArriveTime)</span>
            </td>
            <td class="col-lg-2">
                <span style="font-size: 14px;">@Html.DisplayFor(model => callResponse.FinishTime)</span>
            </td>
            <td class="col-lg-2">
                <span style="font-size: 14px;">@Html.DisplayFor(model => callResponse.ReturnTime)</span>
            </td>
        </tr>
        }
    </table>
</div>